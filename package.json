	"bugs": {
		"url": "https://github.com/TomaszChylinski/MusicHub/issues"
	},
	"homepage": "https://github.com/TomaszChylinski/MusicHub#readme",
	"dependencies": {
		"axios": "^0.19.2",
		"bcryptjs": "^2.4.3",
		"express": "^4.17.1",
		"if-env": "^1.0.4",
		"mongodb": "^3.5.5",
		"mongoose": "^5.9.5",
		"react-router-dom": "^5.1.2"
	},
	"devDependencies": {
		"concurrently": "^5.1.0",
		"nodemon": "^2.0.2"
	}
